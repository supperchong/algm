{
  "version": "0.1.8",
  "license": "MIT",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "engines": {
    "node": ">=10.10.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/supperchong/algm"
  },
  "homepage": "https://algm.dev",
  "keywords": [
    "algorithm",
    "leetcode"
  ],
  "scripts": {
    "start": "tsdx watch",
    "build": "tsdx build --target node",
    "test": "tsdx test",
    "lint": "tsdx lint",
    "prepare": "tsdx build --target node",
    "size": "size-limit",
    "analyze": "size-limit --why",
    "doc": "typedoc --theme ./doc_theme src/index.ts"
  },
  "peerDependencies": {},
  "husky": {
    "hooks": {
      "pre-commit": "tsdx lint"
    }
  },
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "name": "algm",
  "author": "supperchong",
  "module": "dist/algm.esm.js",
  "size-limit": [
    {
      "path": "dist/algm.cjs.production.min.js",
      "limit": "10 KB"
    },
    {
      "path": "dist/algm.esm.js",
      "limit": "10 KB"
    }
  ],
  "devDependencies": {
    "@size-limit/preset-small-lib": "^4.9.1",
    "@typescript-eslint/eslint-plugin": "^4.15.1",
    "@typescript-eslint/parser": "^4.15.1",
    "eslint-config-react-app": "^6.0.0",
    "husky": "^4.3.7",
    "size-limit": "^4.9.1",
    "tsdx": "^0.14.1",
    "tslib": "^2.1.0",
    "typedoc": "^0.20.15",
    "typedoc-default-themes": "^0.12.7",
    "typedoc-plugin-mermaid": "^1.3.0",
    "typescript": "^4.1.5"
  }
}
